$(document).ready(function(){
	  
	
	<% if @parent.has_unconfirmed_post_production_result? %>
		
		$.msgbox("Gagal membuat  Production Progress baru. Masih ada yang belum di konfirmasi.", 
					{type: "error"});
		var $modal = $("#newEntry");
		$modal.modal('hide') ;			
	<% else %>
		<% new_form = escape_javascript(  render :partial => "post_production_results/new_object_form", :locals => {
	    :new_object => @new_object , 
			:url => template_sales_item_post_production_results_url(@parent) 
	  })%>

		$("#newEntry .modal-body").append('<%= new_form %>');

		jQuery('.prevent_double_submit').preventDoubleSubmit();
	<% end %>
		
	
	// $('.datepicker').datepicker({
