$(document).ready(function(){
	  
		<% new_form = escape_javascript(  render :partial => "delivery_entries/new_object_form", :locals => {
	    :new_object => @object , 
			:url => update_delivery_entry_url(@object),
			:customer => @customer  
	  })%>
 
	$("#editEntry .modal-body").append('<%= new_form %>');
	
	jQuery('.prevent_double_submit').preventDoubleSubmit();
});