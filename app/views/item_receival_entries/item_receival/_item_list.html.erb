<table class="table table-bordered table-striped table-highlight" id='tableview'>
	<thead>
		<tr>
			<th>Code Sales Item</th> 
			<th>Jumlah</th> 
			
				<th colspan='2'>Action</th> 
		 
		</tr>
	</thead>
	<tbody> 
		
		<% if not defined?(details)  %>
			<% details = 0 %>
		<% end %>
		<% objects.each do |object| %>
			<%= render :partial =>  'item_receival_entries/entry', :locals => {
				:object => object ,
				:parent => parent ,
				:details => details
			} %> 
		<% end %>
	</tbody>
</table>